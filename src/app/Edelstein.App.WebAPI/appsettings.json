{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console"
        }
      }
    ]
  },
  "AllowedHosts": "*",

  "DataStore": {
    "Type": "PostgreSQL",
    "ConnectionString": "host=localhost;database=edelstein;"
  },
  "Caching": {
    "Type": "Redis",
    "ConnectionString": "localhost"
  },
  "Messaging": {
    "Type": "Redis",
    "ConnectionString": "localhost"
  },
  "Parser": {
    "Type": "NX",
    "Directory": "data"
  }
}
